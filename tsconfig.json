{
  "compilerOptions": {
    "target": "ES2015",
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "noImplicitAny": false,
    "typeRoots": [
      "./types",
      "./node_modules/@types"
    ]
  },
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Recommended"
}